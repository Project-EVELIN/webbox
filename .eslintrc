{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false,
            "forOf": true
        }
    },
    "rules": {
        "semi": 2,
        "space-before-function-paren": 2,
        "indent": [2, 2]
    },
    "env": {
        "browser": true,
        "node": true
    }
}