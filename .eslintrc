{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "forOf": true,
      "experimentalObjectRestSpread": true,
      "generators": true,
      "restParams": true,
      "spread": true,
      "templateStrings": true
    }
  },
  "rules": {
    "semi": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "no-unused-vars": 2,
    "no-console": 0,
    "react/prop-types": 0
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "node": true
  }
}
