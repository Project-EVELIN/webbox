doctype html
html(lang="de")

  meta(charset="utf-8")
  script(src="https://use.fontawesome.com/3797abfac7.js")
  title trycoding.io - Hochschule Coburg
  if isProd
    link(rel="stylesheet", href="/public/css/all.bundle.#{webboxVersion}.css")

  body.ide
    #ide-container

    script.
      window.sourcebox = !{sourcebox};
      window.websocket = !{websocket};
      window.INITIAL_DATA = !{INITIAL_DATA};
      window.USER_DATA = !{USER_DATA};

    script(src="https://cdn.jsdelivr.net/ace/1.2.3/min/ace.js")
    script(src="https://cdn.jsdelivr.net/ace/1.2.3/min/ext-themelist.js")
    script(src="https://cdn.jsdelivr.net/ace/1.2.3/min/ext-modelist.js")
    script(src="https://cdn.jsdelivr.net/d3js/3.5.17/d3.min.js")

    if isProd
      script(src="/public/js/react-commons.bundle.#{webboxVersion}.js")
      script(src="/public/js/embed.bundle.#{webboxVersion}.js")
    else
      script(src="/public/js/react-commons.bundle.js")
      script(src="/public/js/embed.bundle.js")
